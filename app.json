{
  "expo": {
    "name": "Whatnime",
    "slug": "Whatnime",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/MainLogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splashscreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#1f1f1f"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "1.0.0",
      "bundleIdentifier": "com.inozura.whatnime"
    },
    "android": {
      "versionCode": 1,
      "package": "com.inozura.whatnime",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptiveicon.png",
        "backgroundColor": "#1f1f1f"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
